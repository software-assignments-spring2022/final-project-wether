# environment variables for the React front-end 
# you must manually restart the react app to reload any modifications to this file

# react server will use whatever port you specify here
PORT=4000 

# any other variables must be prefixed with REACT_APP_
# access any variable from within React code as process.env.REACT_APP_VARNAME, where VARNAME is replaced with the appropriate variable name

REACT_APP_SERVER_HOSTNAME=http://localhost:3000